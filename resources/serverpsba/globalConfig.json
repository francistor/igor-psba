{
	"sessionCDRDirectories": [
		{
			"path": "/var/yaas/cdr/session",
			"filenamePattern": "cdr_%d{yyyyMMdd}.txt"
		},
		{
			"path": "/var/yaas/cdr/session-hot",
			"filenamePattern": "cdr_copied.txt",
			"checkerName": "filters/copyCDRWriteCheck.json"
		}
	],
	"serviceCDRDirectories": [
		{
			"path": "/var/yaas/cdr/service",
			"filenamePattern": "cdr_%d{yyyyMMdd}.txt"
		}
	],
	"writeSessionCDR": false,
	"writeServiceCDR": false,
	"cdrFilenamePattern": "cdr_%d{yyyyMMdd}.txt",

	"sessionAccountingCopyTargets":[
		{
			"targetName": "session-target-1",
			"RadiusGroupName": "psba-superserver-group",
			"CheckerName": "checkSessionAccounting",
			"FilterName": "filters/passAll.json"
		}
	],

	"serviceAccountingCopyTargets":[
		{
			"targetName": "service-target-1",
			"RadiusGroupName": "psba-superserver-group",
			"CheckerName": "checkServiceAccounting",
			"FilterName": "filters/passAll.json"
		}
	],

	"proxyGroupName": "",
	"acceptOnProxyError": false,
	"proxySessionAccounting": false,
	"proxyServiceAccounting": false,
	"authProxyFilterOut": "filters/standardProxyFilterOut.json",
	"authProxyFilterIn": "filters/standardProxyFilterIn.json",
	"acctProxyFilterOut": "filters/standardProxyFilterOut.json",
	"proxyTimeoutMillis": 500,
	"proxyRetries": 1,

	"provisionType": "file",
	"authLocal": "none",

	"useRejectService": false,
	"rejectServiceName": "void",
	"rejectIsAddon": false,

	"blockingWithService": false,
	"blockingServiceName": "void",
	"blockingIsAddon": false,

	"radiusAttrs":[
		{"Session-Timeout": 3600}
	],
	"nonOverridableRadiusAttrs": [
		{"Cisco-AVPair": "global=true"}
	]
}
