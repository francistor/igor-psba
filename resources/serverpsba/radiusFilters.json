{
    "copyProxy":{
        "remove": ["NAS-Port-Id", "NAS-Identifier", "NAS-IP-Address", "NAS-Port"],
        "force": [
          ["Service-Type", "Call-Check"]
        ]
    },
    "standardProxyIn":{
        "allow": ["Class", "Framed-IP-Address", "Reply-Message", "Framed-Protocol"]
    },
    "standardProxyOut":{
        "remove": ["NAS-Port-Id", "NAS-Identifier", "NAS-IP-Address", "NAS-Port"],
        "force": [
          ["Service-Type", "Framed"]
        ]
    },
    "altProxyOut":{
      "remove": ["NAS-Port-Id", "NAS-Identifier", "NAS-IP-Address", "NAS-Port"],
      "force": [
        ["Service-Type", "Login"]
      ]
  }
}